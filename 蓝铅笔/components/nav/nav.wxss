/* components/nav/nav.wxss */
.headerbox .headers{
    width: 100%;
    height: 80rpx; 
    box-sizing: border-box;
    padding: 10rpx;
    background-color: #fff;
    display: flex;
}

.headers .header-brandImg{
    width: 50%;
    height: 100%;
}
.headers .year{
    margin: 7rpx 0 0 20rpx;
    width: 100rpx;
    height: 45rpx;
    background-image:url("https://assets-cdn.lanqb.com/lanqb/2023/2023year.png");
    background-size:100%  100%;
}
.headers .loingRegisterbox{
    width: 200rpx;
    /* height: 150rpx; */
    /* border: 1px red solid; */
    position: absolute;
    /* top: 0; */
    right: 20rpx;
    /* z-index: 200; */
    padding: 10rpx;
    box-sizing: border-box;
    background-color: #fff;
    animation: animation 2s linear infinite  alternate-reverse ; 
}
.headers .loingRegisterbox .loingRegisterImg{
    background-image:url("https://assets-cdn.lanqb.com/pc/login/regster-reward_3.png");
    background-size: 100%;
    /* border: 1px red solid; */
    height: 55rpx;
    border-radius: 20rpx;
}
.headers .loingRegisterbox .loingRegisterboxs{
    display: flex;
    justify-content: space-between;
    font-weight: bold;
    font-size: 38rpx;
    margin-top: 20rpx;
}
@keyframes animation{
    0%,45%{top: 0 ;}
    55%,100%{top: -90% ;}
}
