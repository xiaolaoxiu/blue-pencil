/* components/bottomNav/bottomNav.wxss */
.bottomNav .bottomNavs{
    width: 100%;
    height: 250rpx;
    text-align: center;
    color: rgba(0, 0, 0, 0.397);
    padding: 20rpx;
    box-sizing: border-box;  
    background-color: rgba(173, 173, 173, 0.027);
}
.bottomNav .bottomNavs .bottomNavsText text{ 
    font-size: 22rpx;
    padding: 0 20rpx;
    border-left: 1px rgba(0, 0, 0, 0.397) solid;
    border-right: 1px rgba(0, 0, 0, 0.397) solid;
}
.bottomNav .bottomNavs .credible{
    width: 100%;
    height: 60rpx;
    display: flex;
    justify-content: space-between;
    margin-top: 20rpx;
}
.bottomNav .bottomNavs .credible view{
    width: 100rpx;
    height: 40rpx;
    margin: 8rpx;
}
.Bright{
    line-height: 35rpx;
    position: relative;
}
.bottomNav .bottomNavs .Bright text{
    background-image: url('https://assets-cdn.lanqb.com/image/footer/footer-icon4@2x.png');
    width: 20rpx;
    height: 20rpx;
    display: block;
    position: absolute;
    background-size: 100%;
    left: 200rpx;
    top: 8rpx;
}



