/* 子菜单 */
.submenu{
    width: 100%;
    height: 320rpx;
    display: flex;
    flex-wrap: wrap;
    background-color: #fff;
}
.submenu .submenus{
    width: 90rpx;
    height: 90rpx;
    margin: 15rpx 48rpx;
    border-radius: 50%;
}
.submenu .submenus image {
    border-radius: 50%;
}
.submenu .submenus view{
    text-align: center;
    font-size: 22rpx;
}