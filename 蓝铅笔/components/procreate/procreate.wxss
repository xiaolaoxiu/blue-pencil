    padding: 10rpx;
    box-sizing: border-box;
}
.procreatebox .procreates .procreatesImg{
    width: 35%;
    height: 100%;
    /* border: 1px red solid; */
}
.procreatebox .procreates .procreatesText{
    width: 65%;
    height: 100%;
    /* border: 1px red solid; */
