    padding: 10rpx;
    box-sizing: border-box;
}

.procreatebox .procreates .procreatesText .procreatesTextone{
    width: 100%;
    height: 40%;
    /* border: 1px red solid; */
    font-size: 30rpx;
    /* font-weight: 600; */
