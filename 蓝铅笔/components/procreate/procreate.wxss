}


.procreatebox .procreates .procreatesText .procreatesTexttwo{
    width: 100%;
    /* height: 40%; */
    /* border: 1px red solid; */
    display: flex;
    /* padding-left: 20rpx; */
    justify-content: space-between;
    margin: 10rpx 0;
    font-size: 28rpx;
    /* font-weight: 600; */
}
.procreatebox .procreates .procreatesText .procreatesTexttwo text{
    margin-left:70rpx ;
}
.procreatebox .procreates .procreatesText .procreatesTexttwo image{
    position: absolute;
    /* border: solid 1px red ; */
    /* display: block; */
    width: 40rpx;
    height: 40rpx;
    left: 20rpx;
    border-radius:50%;
}
.procreatesTextthreen{
    color: rgba(0, 0, 0, 0.548);
    font-size: 27rpx;
}
.procreatesTextthreen .TextsBox  {
    position: absolute;
    right:10rpx;
    top: 130rpx;
    display: flex;
    border: orangered 3rpx solid;
    border-radius: 15rpx;
    line-height: 45rpx;
    justify-content: space-between;
    overflow: hidden;
    text-align: center;
    /* padding: 1rpx; */
}
.TextsBoxs:nth-child(1){
    color: orangered;
    padding: 5rpx;
}
.TextsBoxs:nth-child(2){
    background-color: orangered;
    color: #fff;
    padding: 5rpx;
}